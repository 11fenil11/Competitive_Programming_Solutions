#include <stdio.h>

int main(void) {
	int T,N,S,X,a,b;
	scanf("%d",&T);
	while(T--)
	{
             scanf("%d %d %d",&N,&X,&S);
		
	while(S--)
	 {
              scanf("%d %d",&a,&b);
	  if(X==a)
	         { 
		X=b;
		
	          }
	else if(X==b)
	         {
		X=a;
		
	         }}
		printf("%d\n",X);
	  
	}
	 return 0;
}
